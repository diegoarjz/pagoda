create_box = CreateBoxGeometry{
    width: 10,
    depth: 10,
    height: 5
}

split = Split{
    axis: "x",
    split_count: 3,
    split_1: 1,
    split_2: 3,
    split_3: 6
}

export_geometry = ExportGeometry{
    path: $< "regression_results/split/geometry" + op.count + ".obj"; >$
}

create_box>out -> in<split;
split>split_1 -> in<export_geometry;
split>split_2 -> in<export_geometry;
split>split_3 -> in<export_geometry;
