create_box = CreateBoxGeometry {
    width: 20,
    depth: 20,
    height: 20
}

repeat_split_x = RepeatSplit {
    size: 5,
    adjust: false,
    axis: "x"
}

repeat_split_y = RepeatSplit {
    size: 5,
    adjust: false,
    axis: "y"
}

repeat_split_z = RepeatSplit {
    size: 10,
    adjust: false,
    axis: "z"
}


export_geometry = ExportGeometry {
    path: $< "regression_results/repeat_split/geometry_" + op.count + ".obj"; >$
}

create_box>out -> in<repeat_split_x;
repeat_split_x>out -> in<repeat_split_y;
repeat_split_y>out -> in<repeat_split_z;
repeat_split_z>out -> in<export_geometry;
