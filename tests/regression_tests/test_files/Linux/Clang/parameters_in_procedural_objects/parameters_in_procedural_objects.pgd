create_rect = CreateRectGeometry{
    width: 50,
    height: 10
}

repeat_split = RepeatSplit {
    size: 5,
    adjust: true,
    axis: "x"
}

extrude = ExtrudeGeometry {
    amount: $< in.index; >$
}

export_geometry = ExportGeometry {
    path: $< "regression_results/parameters_in_procedural_objects/geometry_" + count + ".obj"; >$
}

create_rect>out -> in<repeat_split;
repeat_split>out -> in<extrude;
extrude>out -> in<export_geometry;
