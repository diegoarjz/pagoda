create_box = CreateBoxGeometry {
    width: 10,
    depth: 10,
    height: 5
}

rotate = Rotate {
    x: 45,
    y: 0,
    z: 0,
    rotation_order: "x",
    world: true
}

rotate2 = Rotate {
    x: 0,
    y: 45,
    z: 0,
    rotation_order: "y",
    world: true
}

export_geometry = ExportGeometry {
    path: "regression_results/rotate/geometry.obj"
}

create_box>out -> in<rotate;
rotate>out -> in<rotate2;
rotate2>out -> in<export_geometry;
