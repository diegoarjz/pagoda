create_rect = CreateRectGeometry {
    width: 10,
    height: 10
}

extrusion = ExtrudeGeometry {
    amount: 10
}

clip_geometry = ClipGeometry {
    plane: $< Plane(Vector3(0, 0, 4), Vector3(1, 2, 3)); >$
}

export_front_geometry = ExportGeometry {
    path: "regression_results/clip_geometry/front.obj"
}

export_back_geometry = ExportGeometry {
    path: "regression_results/clip_geometry/back.obj"
}

extract_scope = ExtractScope {}

export_scope = ExportGeometry {
    path: $< "regression_results/clip_geometry/scope" + op.count + ".obj"; >$
}

create_rect>out -> in<extrusion>out -> in<clip_geometry;
clip_geometry>front -> in<export_front_geometry;
clip_geometry>back -> in<export_back_geometry;
clip_geometry>front -> in<extract_scope;
clip_geometry>back -> in<extract_scope;
extract_scope>out -> in<export_scope;
