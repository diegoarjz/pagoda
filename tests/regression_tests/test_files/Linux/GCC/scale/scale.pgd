create_box = CreateBoxGeometry {
    width: 10,
    depth: 10,
    height: 10
}

translate_scope_origin = Translate {
    x: 1,
    y: 1,
    z: 1,
    world: true
}

translate_scope_center = Translate {
    x: 1,
    y: 1,
    z: 1,
    world: true
}

translate_world_origin = Translate {
    x: 1,
    y: 1,
    z: 1,
    world: true
}

scale_scope_origin = Scale {
    x: 0.1,
    y: 0.1,
    z: 0.1,
    scale_center: "scope_origin"
}

scale_scope_center = Scale {
    x: 0.1,
    y: 0.1,
    z: 0.1,
    scale_center: "scope_center"
}

scale_world_origin = Scale {
    x: 0.1,
    y: 0.1,
    z: 0.1,
    scale_center: "world_origin"
}

export_scope_origin = ExportGeometry {
    path: "regression_results/scale/geometry_scope_origin.obj"
}

export_scope_center = ExportGeometry {
    path: "regression_results/scale/geometry_scope_center.obj"
}

export_world_origin = ExportGeometry {
    path: "regression_results/scale/geometry_world_origin.obj"
}

create_box>out ->in<translate_scope_origin;
translate_scope_origin>out -> in<scale_scope_origin;
scale_scope_origin>out -> in<export_scope_origin;

create_box>out -> in<translate_scope_center;
translate_scope_center>out -> in<scale_scope_center;
scale_scope_center>out -> in<export_scope_center;

create_box>out -> in<translate_world_origin;
translate_world_origin>out -> in<scale_world_origin;
scale_world_origin>out -> in<export_world_origin;
