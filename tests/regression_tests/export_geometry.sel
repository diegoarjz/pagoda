create_rect = Operation(operation: "CreateRectGeometry") {
    width: 10, height: 10
}
create_rect_out = OutputInterface(interface: "out", offset: 0)
extrusion_in = InputInterface(interface: "in", offset: 0)
extrusion = Operation(operation: "ExtrudeGeometry") {extrusion_amount: 10}
extrusion_out = OutputInterface(interface: "out", offset: 0)

export_geometry_in = InputInterface(interface: "in", offset: 0)
export_geometry = Operation(operation: "ExportGeometry", offset: 0) {path: $< "geometry_" + op.count + ".obj" >$}

create_rect -> create_rect_out -> extrusion_in -> extrusion -> extrusion_out;

export_geometry_in -> export_geometry;
create_rect_out -> export_geometry_in;
extrusion_out -> export_geometry_in;